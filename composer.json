{
  "name": "oriceon/laravel-assets",
  "keywords": [
    "assets",
    "minify",
    "laravel"
  ],
  "description": "A package for minify or combine css and javascript files",
  "license": "MIT",
  "authors": [
    {
      "name": "Mubashar Ahmad",
      "email": "info@zanysoft.com"
    },
    {
      "name": "Valentin IvaÈ™cu",
      "email": "oriceon@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "intervention/image": "^2.1"
  },
  "autoload": {
    "files": [
      "src/Helpers/helpers.php"
    ],
    "psr-4": {
      "ZanySoft\\LaravelAssets\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\LaravelAssets\\AssetsServiceProvider"
      ],
      "aliases": {
        "Assets": "ZanySoft\\LaravelAssets\\Facade"
      }
    }
  },
  "minimum-stability": "stable"
}
